<slickboard>

   <object>
      <slide duration='8' />
      <transition_in type='fade' duration='.5' /> 
      <transition_out type='fade' duration='.1' /> 
      
      <object id='group_1'>
         <rect x='0' y='0' width='480' height='300' fill_color='FBFBFA' xblend='difference' />
         <text x='20' y='0' size='35' color='000088' alpha='.7' shadow='default'>More</text>
      </object>
      
      <object id='group_2'>
         <text x='20' y='30' size='100' color='000000' alpha='.7' shadow='default'>Simply</text>
         <transition_out type='fly' startPoint='8' easing='normal' delay='4' duration='.5' />
      </object>
      
      <object id='group_3'>
         <text x='20' y='30' size='100' color='006633' alpha='.8' shadow='default'>Ecology</text>
         <transition_in type='fly' startPoint='2' easing='normal' delay='4' duration='.5' /> 
      </object>
      
   </object>
   
   <!-- first slide -->
   <object>
      <image url='./css/img/demo/image01.jpg' x='0' y='0' width='240' height='150' shadow='default' />
      <slide duration='.5' />
      <transition_in type='fly' duration='1' startPoint='6' />
      <transition_out type='fly' duration='1' startPoint='4' />
   </object>

   <!-- second slide (stacked) -->
   <object>
      <image url='./css/img/demo/image02.jpg' x='240' y='0' width='240' height='150' shadow='default' />
      <slide duration='.5' stack='true' />
      <transition_in type='fly' duration='1' startPoint='6' />
      <transition_out type='fly' duration='1' startPoint='4' />
   </object>
   
   <!-- third slide (stacked) -->
   <object>
      <image url='./css/img/demo/image03.jpg' x='0' y='0' width='240' height='150' shadow='default' />
      <slide duration='.5' stack='true' />
      <transition_in type='fly' easing='bounce' startPoint='2' />
      <transition_out type='fly' duration='1' startPoint='4' />
   </object>

   <!-- fourth slide (stacked) -->
   <object>
      <image url='./css/img/demo/image04.jpg' x='240' y='0' width='240' height='150' shadow='default' />
      <slide duration='1' stack='true' />
      <transition_in type='blinds' duration='1' startPoint='6' />
      <transition_out type='fly' duration='5' startPoint='4' />
   </object>

   <object>
      <slide duration='15' />
      <rect x='0' y='0' width='480' height='150' fill_color='FBFBFA' />
      <image url='./css/img/demo/image05.jpg' fill='trim' width='480' height='150' shadow='default' />
      <text x='20' y='0' size='50' color='000088' shadow='default'>Enjoy Swimming</text>
      <text x='200' y='60' size='30' color='FFFFFF' shadow='default'>&</text>
      <text x='20' y='90' size='50' color='006633' shadow='default'>Change Your Life</text>
      <transition_in type='zoom' duration='.5' />  
   </object>

</slickboard>
